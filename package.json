{
  "name": "tiendamayoral",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "add": "^2.0.6",
    "bluebird": "^3.7.2",
    "bootstrap": "^4.6.0",
    "electron-is-dev": "^1.2.0",
    "electron-log": "^4.3.1",
    "electron-pos-printer": "^1.2.4",
    "electron-updater": "^4.3.5",
    "i18next": "^19.8.9",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-http-backend": "^1.1.0",
    "i18next-xhr-backend": "^3.2.2",
    "node-gyp": "^7.1.2",
    "node-pre-gyp": "^0.17.0",
    "react": "^17.0.1",
    "react-bootstrap": "^1.4.3",
    "react-devtools": "^4.10.1",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.7",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "sqlite3": "^5.0.2",
    "web-vitals": "^1.0.1",
    "yarn": "^1.22.10"
  },
  "scripts": {
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "start-electron": "nodemon --watch ./public/* --exec \"electron .\"",
    "dev": "concurrently \"yarn start-react\" \"wait-on http://localhost:3000 && yarn start-electron\" ",
    "postinstall": "electron-builder install-app-deps",
    "pack-app": "yarn build-react && electron-builder --publish never",
    "pack-app-mac": "yarn build-react && electron-builder --publish always",
    "pack-app-win": "yarn build-react && electron-builder --win --x64 --publish always",
    "pack-app-win-never": "yarn build-react && electron-builder --win --x64 --publish never",
    "pack-build": "electron-builder --dir",
    "electron-pack": "electron-builder --win -c.extraMetadata.main=build/electron.js",
    "build": "yarn build-react && electron-builder",
    "test": "react-scripts test",
    "publish": "electron-builder --publish always",
    "release": "build",
    "eject": "react-scripts eject"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "build": {
    "appId": "com.mayoral.SqliteReact",
    "productName": "Package-sqlite",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "win": {
      "publish": [
        "github"
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "extraResources": [
      {
        "from": "./db",
        "to": "db"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IvanGonzalezSuarez/pruebaSqlite.git"
  },
  "publish": {
    "provider": "github",
    "releaseType": "release"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "electron": "^11.2.3",
    "electron-builder": "^22.9.1",
    "nodemon": "^2.0.7",
    "wait-on": "^5.2.1"
  }
}